{
  "name": "snack-adventure-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:vite dev:tailwind",
    "dev:vite": "vite",
    "dev:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "build": "tsc -b && npm run tailwind:build && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tailwind:build": "npx tailwindcss -i ./src/index.css -o ./src/output.css --minify",
    "tailwind:watch": "npx tailwindcss -i ./src/index.css -o ./src/output.css --watch"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.2.0",
    "framer-motion": "^12.23.16",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}
